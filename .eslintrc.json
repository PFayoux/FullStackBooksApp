{
  "extends": ["react-app", "react-app/jest", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      },
      {
        "usePrettierrc": true
      }
    ],
    "max-len": [
      "error", {
        "code": 80,
        "tabWidth": 2
      }
    ]
  }
}
